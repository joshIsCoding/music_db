<% notes.each do |note| %>
   <article class="note">
      <header>
         <h6>
            Posted by <%= note.email %> on 
            <time datetime="<%= note.created_at %>"><%= note.created_at %></time>
         </h6>
      </header>
      <p><%= note.content %></p>
      <footer>
         <% if note.user_id == current_user.id %>
            <%= button_to "Delete", note_url(note), method: "delete" %>
         <% end %>
      </footer>
      
   </article>
<% end %>